<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
	<key>Label</key>
	<string>org.pkgsrc.lpjs_compd</string>
	<key>ProgramArguments</key>
	<array>
	    <string>/opt/pkg/sbin/lpjs_compd</string>
	    <string>--log-output</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<!-- Must be true, or launch will fail if it can't connect -->
	<!-- Unfortunately also restarts automatically when terminated -->
	<key>KeepAlive</key>
	<true/>
    </dict>
</plist>
