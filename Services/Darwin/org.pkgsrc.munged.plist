<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.pkgsrc.munge</string>
	<key>ProcessType</key>
	<string>Interactive</string>
	<key>EnableTransactions</key>
	<true/>
	<key>TransactionTimeLimitEnabled</key>
	<false/>
	<key>ExitTimeOut</key>
	<integer>60</integer>
	<key>KeepAlive</key>
	<dict>
		<key>PathState</key>
		<dict>
			<key>/usr/pkgsrc/var/spool/munge/cache/org.pkgsrc.munge</key>
			<true/>
		</dict>
	</dict>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/pkg/sbin/munged</string>
		<string>--key-file=/usr/pkg/etc/munge/munge.key</string>
	</array>
	<key>Sockets</key>
	<dict>
		<key>Listeners</key>
		<array>
			<dict>
				<key>SockPathMode</key>
				<integer></integer>
				<key>SockPathName</key>
				<string>/usr/pkg/var/run/munge</string>
			</dict>
		</array>
	</dict>
</dict>
</plist>
